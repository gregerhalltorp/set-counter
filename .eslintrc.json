{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["mocha"],
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "import/extensions": "off",
    "no-bitwise": "off",
    "max-len": [1, 100, 4],
    "arrow-body-style": [0],
    "import/prefer-default-export": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_embedded", "_links", "_source", "_id", "_object", "_keys"]
      }
    ],
    "react/no-unused-prop-types": ["error", { "customValidators": [], "skipShapeProps": true }],
    "no-plusplus": 0,
    "react/jsx-filename-extension": 1,
    "no-self-compare": 0
  }
}
